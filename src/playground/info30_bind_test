/* Bind testing 
const obj = {
    name: 'Vikram', 
    getName() {
        return this.name; 
    }
};  

//console.log(obj.getName());   // works as a class method
//const getName = obj.getName;      // fails as when copied, getName is created as a regular function where 'this' is undefined 
